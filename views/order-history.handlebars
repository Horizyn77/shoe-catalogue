<header>
    <div class="container">
        <nav>
            <h1 class="home-btn">SHOE PARADISE</h1>
        </nav>
    </div>
</header>
<div class="container">
    <div class="cart-menu">
        <h2 id="account-details-heading">Orders</h2>
        <div class="back-container">
            <a href="/user">
                <img src="/images/icons/back.png" alt="">
                <div id="back-button">Back to Home</div>
            </a>
        </div>
    </div>
</div>
<div>
    {{#if orders}}
    {{#each orders}}
    {{#each this}}
    <div>{{this.cart_id}}
        <div></div>
    </div>
    <div>{{this.name}}</div>
    <div>{{this.price}}</div>
    {{/each}}
    {{/each}}
    {{else}}
    <div>No orders</div>
    {{/if}}
</div>
<script src="/js/orders.js"></script>